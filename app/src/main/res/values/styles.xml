<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Fragment_Dialog_Style">
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <style name="personal_center_item_common_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/personal_center_item_height</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="divider_line_common_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_horizontal_height</item>
        <item name="android:background">@color/divider_line</item>
    </style>

    <style name="divider_line_personal_center_item" parent="divider_line_common_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_horizontal_height</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/divider_line</item>
    </style>

</resources>
